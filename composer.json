{
    "name": "pato05/amphp-clamav",
    "description": "An asynchronous clamd wrapper written with amphp/socket",
    "type": "library",
    "require": {
        "phabel/phabel": "^1.0",
        "php": "*",
        "ext-sockets": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amp\\ClamAV\\": "lib/"
        },
        "files": [
            "___transpiledWithPhabel.php"
        ]
    },
    "authors": [
        {
            "name": "Pato05",
            "email": "pato05mc@gmail.com"
        }
    ],
    "require-dev": {
        "amphp/php-cs-fixer-config": "dev-master",
        "amphp/file": "^2.0",
        "phabel/phabel": "1.0.49.80"
    },
    "config": {
        "allow-plugins": {
            "phabel/phabel": true
        }
    },
    "extra": {
        "phabel": {
            "revision": 0,
            "require": {
                "amphp/amp": "^2.6",
                "amphp/socket": "^1.2"
            }
        }
    }
}